{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "FastAPI: Debug",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app:app",
        "--reload",
        "--host",
        "0.0.0.0",
        "--port",
        "8000"
      ],
      "cwd": "${workspaceFolder}/src",
      "jinja": true,
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    },
    {
      "name": "Worker: Debug",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/worker_main.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "jinja": true,
      "justMyCode": false
    }
  ]
}
